#!groovy

@Library("devops") _ 

def ansible = new org.devops.deploy()

String deployHosts = "${env.deployHosts}"

pipeline{
   agent { node { label "master" }}
   
  stages{
   
       stage("build"){
            steps{
                script{
                  ansible.AnsibleDeploy("${deployHosts}","-m ping")
 
                }  
           }
       }
   }
  
}
